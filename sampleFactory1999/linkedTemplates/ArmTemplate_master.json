{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sampleFactory1999"
		},
		"SampleDataInput_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SampleDataInput'"
		},
		"SampleDataOutput_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SampleDataOutput'"
		},
		"Selfhostedserver_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Selfhostedserver'"
		},
		"trigger1_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/186109fb-871b-418a-9a9a-d1e325db4cc5/resourceGroups/PrajitSampleDemo/providers/Microsoft.Storage/storageAccounts/sampledatademo"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sampleFactory1999_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SampleDataInput_connectionString": {
						"value": "[parameters('SampleDataInput_connectionString')]"
					},
					"SampleDataOutput_connectionString": {
						"value": "[parameters('SampleDataOutput_connectionString')]"
					},
					"Selfhostedserver_connectionString": {
						"value": "[parameters('Selfhostedserver_connectionString')]"
					},
					"trigger1_properties_typeProperties_scope": {
						"value": "[parameters('trigger1_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}